{
    "name": "bishwas-portfolio-contact",
    "description": "Contact form for Bishwas Ghimire portfolio website",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "phpmailer/phpmailer": "^6.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Portfolio\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "php-cs-fixer fix"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}